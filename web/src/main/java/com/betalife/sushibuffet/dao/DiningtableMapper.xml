<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.betalife.sushibuffet.dao.DiningtableMapper">

  <cache />

  	<select id="selectTables" resultType="Diningtable">
		SELECT d.id, d.available, t.checkout
		FROM diningtable d LEFT JOIN turnovers t ON d.id = t.table_id
	</select>

</mapper>