<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.betalife.sushibuffet.dao.TaxgroupsMapper">
	<cache flushInterval="6000000" />

	<select id="selectAll" resultType="Taxgroups" useCache="true">
		SELECT
		id,
		name,
		value,
		takeaway
		FROM taxgroups
	</select>


</mapper>